!!!
%html
  %head
    %meta{:content => "text/html; charset=UTF-8", "http-equiv" => "Content-Type"}/
    %title Mapping115
    = csrf_meta_tags
    = favicon_link_tag
    = csp_meta_tag
    %link{:crossorigin => "anonymous", :href => "https://use.fontawesome.com/releases/v5.7.0/css/all.css", :integrity => "sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ", :rel => "stylesheet"}
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_pack_tag 'application', 'data-turbolinks-track': 'reload'

    %link{:href => "https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/themes/default/style.min.css", :rel => "stylesheet"}/
    %script{:src => "https://cdnjs.cloudflare.com/ajax/libs/jstree/3.2.1/jstree.min.js"}
    %script{:src => "https://cdn.jsdelivr.net/npm/chart.js@2.8.0"}

  %body{:id => "#{controller_name}_#{action_name}"}
    = render "shared/flash"
    = yield
    = javascript_include_tag "moment"
    = javascript_include_tag "daterangepicker"
